<?xml version="1.0"?>
<form xmlns="http://www.demandware.com/xml/form/2008-04-19">
â€‹
    <field 
        formid="firstname" 
        label="label.input.firstname.profile" 
        mandatory="true" 
        max-length="50"
        missing-error="error.message.required" 
        range-error="error.message.50orless" 
        type="string" 
    />

    <field 
        formid="lastname" 
        label="label.input.lastname.profile" 
        mandatory="true" 
        max-length="50" 
        missing-error="error.message.required"
        range-error="error.message.50orless" 
        type="string" 
    />

    <field
        formid="email" 
        label="label.input.email.profile" 
        mandatory="true" 
        max-length="50" 
        missing-error="error.message.required"
        parse-error="error.message.parse.email.profile.form"
        range-error="error.message.50orless"
        regexp="^[\w.%+-]+@[\w.-]+\.[\w]{2,6}$" 
        value-error="ValueErrorText"
        type="string"
    />

    <field 
        formid="storename" 
        label="label.input.feirante.storename" 
        mandatory="true" 
        max-length="50" 
        missing-error="error.message.required"
        range-error="error.message.50orless" 
        binding="storename"
        type="string" 
    />

    <field 
        formid="city" 
        label="label.input.feirante.city" 
        mandatory="true" 
        max-length="50" 
        missing-error="error.message.required"
        range-error="error.message.50orless" 
        type="string" 
    />

    <field 
        formid="userId" 
        mandatory="true" 
        max-length="50" 
        type="number" 
    />

    <field 
        formid="feirante" 
        max-length="50" 
        type="string" 
    />
</form>